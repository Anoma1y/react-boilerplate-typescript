  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --env=production --config=internals/webpack/webpack.prod.config.js --mode=production --color -p --progress --hide-modules --display-optimization-bailout",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --env=development --config=internals/webpack/webpack.dev.config.js --mode=development --port 8080",
    "generate": "plop --plopfile internals/generators/index.js",
    "generate:reducers": "node ./internals/helpers/ReducersGenerator.js",
    "prettier:base": "./node_modules/.bin/prettier --config .prettierrc",
    "prettier:all": "yarn prettier:base --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:ts": "yarn prettier:base --write \"**/*.{ts,tsx}\"",
    "prettier:js": "yarn prettier:base --write \"**/*.{js,jsx}\"",
    "prettier:json": "yarn prettier:base --write *.json",
    "stylelint:scss": "./node_modules/.bin/stylelint './app/**/*.{scss,sass}'; exit 0",
    "tslint": "./node_modules/.bin/tslint --config tslint.json --project tsconfig.json"
  },
