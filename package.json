{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:clean": "rimraf ./build",
    "build:public": "node ./internals/helpers/CopyToBuild.js",
    "build": "yarn build:clean && yarn build:public && cross-env NODE_ENV=production webpack --env=production --config=internals/webpack/webpack.prod.config.js --mode=production --color -p --progress --hide-modules --display-optimization-bailout",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --env=development --config=internals/webpack/webpack.dev.config.js --mode=development --port 8080",
    "lint": "yarn run lint:js",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "yarn run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "prettify": "prettier --write app/**/*.js"
  },
  "author": "Anoma1y",
  "license": "ISC",
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
