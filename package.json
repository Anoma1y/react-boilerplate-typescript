{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn generate:all && yarn test:all && cross-env NODE_ENV=production webpack --env=production --config=internals/webpack/webpack.prod.config.js --mode=production --color -p --progress --hide-modules --display-optimization-bailout",
    "build:force": "cross-env NODE_ENV=production webpack --env=production --config=internals/webpack/webpack.prod.config.js --mode=production --color -p --progress --hide-modules --display-optimization-bailout",
    "dev": "yarn generate:all && cross-env NODE_ENV=development webpack-dev-server --env=development --config=internals/webpack/webpack.dev.config.js --mode=development --port 8080",
    "generate": "plop --plopfile internals/generators/index.js",
    "generate:all": "node ./internals/helpers/ApiGenerator.js && node ./internals/helpers/ReducersGenerator.js",
    "generate:reducers": "node ./internals/helpers/ReducersGenerator.js",
    "generate:api": "node ./internals/helpers/ApiGenerator.js",
    "stylelint:scss": "./node_modules/.bin/stylelint './app/**/*.{scss,sass}'; exit 0",
    "test:all": "yarn test:utils",
    "test:utils": "./node_modules/.bin/jest './app/utils/tests'",
    "tslint": "./node_modules/.bin/tslint --config tslint.json --project tsconfig.json"
  },
  "author": "Anoma1y",
